package com.tracy.test;

import java.util.Scanner;

public class HelloWorld {
	public static void main(String[] args) 
	{	int n=0;
		int i;
		int j;
		System.out.println("100以内的素数有"+n+"个");
		for(i=1;i<=100;i++)
		{
			for(j=i;j>1;j--)
				if(i%(j-1)==0)
					break;
			if(j==2)
			System.out.println(i);
				
		}
	}

}
